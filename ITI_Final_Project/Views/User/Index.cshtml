
@{
    ViewBag.title = "Index";
}
@model List<Customer>
<h1>Profile</h1>
 
<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>
                Name
            </th>
            <th>
                Nationality
            </th>
            <th>
                Phone_Number
            </th>
            <th>
                Notional_Number
            </th>
            <th>
                Email
            </th>
        </tr>
    </thead>
    <tbody>
        <tr>
            @foreach (var item in Model)
            {


                <td>
                    @item.Name
                </td>

                @*<td>
                    @item.Nationality
                </td>*@

                <td>
                    @item.Phone_Number
                </td>

                <td>
                    @item.Notional_Number
                </td>


                <td>
                    @item.Email
                </td>


            }
        </tr>
    </tbody>
</table>

